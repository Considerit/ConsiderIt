:javascript
  $(document).ready(function(){

    ConsiderIt.public_root = "#{@public_root}"

    ConsiderIt.proposals = #{@proposals.to_json};

    ConsiderIt.positions = #{@positions.to_json};

    ConsiderIt.current_proposal = #{ @current_proposal ? @current_proposal : 'null' };

    ConsiderIt.current_point = #{ @current_point ? @current_point : 'null' };

    ConsiderIt.inaccessible_proposal = #{ @inaccessible_proposal ? @inaccessible_proposal.to_json : 'null' };

    //var users = { User.select("id, name, avatar_file_name").to_json };
    ConsiderIt.users = #{@users};

    ConsiderIt.current_tenant_data = {
        id : "#{current_tenant.id}",
        identifier : "#{current_tenant.identifier}",
        assessment_enabled : #{current_tenant.assessment_enabled},
        enable_user_conversations : #{current_tenant.enable_user_conversations},
        slider_right : "#{current_tenant.slider_right}", 
        slider_left : "#{current_tenant.slider_left}", 
        slider_prompt : "#{current_tenant.slider_prompt}", 
        considerations_prompt : "#{current_tenant.considerations_prompt}", 
        statement_prompt : "#{current_tenant.statement_prompt}", 
        pro_label : "#{current_tenant.pro_label}",
        header_text : "#{current_tenant.header_text}",
        header_details_text : "#{current_tenant.header_details_text}",        
        con_label : "#{current_tenant.con_label}",
        pledge_enabled : #{current_tenant.requires_civility_pledge_on_registration},
        project_url : "#{current_tenant.project_url}",
        enable_moderation : #{current_tenant.enable_moderation}, 
        moderate_points_mode : #{current_tenant.moderate_points_mode}, 
        moderate_comments_mode : #{current_tenant.moderate_comments_mode}, 
        moderate_proposals_mode : #{current_tenant.moderate_proposals_mode},
        num_proposals_per_page : #{ current_tenant.num_proposals_per_page() },
        homepage_pic_remote_url : "#{ current_tenant.homepage_pic_remote_url }",
        homepage_pic_file_name : "#{ current_tenant.homepage_pic_file_name }"
    };


    // CURRENT USER
    ConsiderIt.current_user = #{current_user ? current_user.id : 'null'};

    #{current_user ? "ConsiderIt.current_user_data = { user: #{current_user.to_json(:methods => :third_party_authenticated)}.user, follows: #{current_user.follows.all.to_json} }" : ''}
    
    // LIMITED USER

    var limited_user_data = #{@limited_user_email ? "{email: '#{@limited_user_email}'}" : 'null'};

    ConsiderIt.limited_user_data = #{@limited_user ? "{ id: #{@limited_user.id}, email: '#{@limited_user_email}', google_uid: '#{@limited_user.google_uid}', facebook_uid: '#{@limited_user.facebook_uid}', twitter_uid: '#{@limited_user.twitter_uid}'}" : 'limited_user_data'};

    ConsiderIt.limited_user_follows = #{@limited_user ? @limited_user_follows.to_json : 'null'};


    // AUTH
    ConsiderIt.password_reset_token = #{@reset_password_token ? "'#{@reset_password_token}'" : 'null'};  

    ConsiderIt.admin_files = {
      js_tag : "#{j(javascript_include_tag("admin"))}",
      style_tag : "#{j(stylesheet_link_tag("admin"))}"
    };

  });

