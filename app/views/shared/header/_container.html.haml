#masthead.header
      
  -# = render :partial => "shared/available_themes"

  .logo_cell
    %a.logo_home{ :href => root_path, :title => ""}
      = image_tag current_theme_image_path "logo.png"
        
  %ul#nav-user
    - if user_signed_in?
      = render :partial => "shared/header/logged_in_head"
      
    - else
      = render :partial => "shared/header/logged_out_head"
        