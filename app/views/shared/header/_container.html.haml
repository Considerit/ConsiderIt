

#masthead.header
      
  -# = render :partial => "shared/available_themes"
  .inner
    .logo_cell
      %a.logo_home{ :href => root_path, :title => ""}
        :javascript
          function no_image(source) {
            $(this).parents('.logo_cell').remove();
          }
        %img#header_logo{ :src => current_theme_image_path("logo.png"), :onerror => "no_image(this);", :title => "Go back to the homepage" }
          
    - if @proposal && @can_update && false
      = render :partial => "proposals/admin_strip"
          
    #nav-user
      -# if user_signed_in?
        = render :partial => "shared/header/logged_in_head"
        
      -# else
        = render :partial => "shared/header/logged_out_head"
